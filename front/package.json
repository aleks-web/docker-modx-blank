{
	"name": "myapp",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"test:unit": "vitest",
		"test": "npm run test:unit -- --run && npm run test:e2e",
		"test:e2e": "playwright test",
		"typeorm": "tsx ./node_modules/typeorm/cli.js",
		"db:mg.gen": "npm run typeorm -- --dataSource=src/lib/server/database/ormconfig.ts migration:generate ./src/lib/server/database/migrations/sync",
		"db:mg.new": "npm run typeorm migration:create ./src/lib/server/database/migrations/$MIGRATION_NAME",
		"db:mg.run": "npm run typeorm -- --dataSource=src/lib/server/database/ormconfig.ts migration:run",
		"db:mg.revert": "npm run typeorm -- --dataSource=src/lib/server/database/ormconfig.ts migration:revert",
		"db:seed": "tsx ./src/lib/server/database/seeds/seed",
		"db:drop": "tsx ./src/lib/server/database/seeds/dropDb",
		"db:recreate": "npm run db:drop && npm run db:sync && npm run db:seed",
		"db:sync": "npm run typeorm -- --dataSource=src/lib/server/database/ormconfig.ts schema:sync",
		"doc:up": "docker-compose up -d"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@fontsource/fira-mono": "^5.0.0",
		"@neoconfetti/svelte": "^2.0.0",
		"@playwright/test": "^1.49.1",
		"@sveltejs/adapter-node": "^5.2.11",
		"@sveltejs/kit": "^2.16.0",
		"@sveltejs/vite-plugin-svelte": "^5.0.0",
		"@tailwindcss/vite": "^4.0.0",
		"@testing-library/jest-dom": "^6.6.3",
		"@testing-library/svelte": "^5.2.4",
		"@types/lodash": "^4.17.16",
		"cross-env": "^7.0.3",
		"eslint": "^9.18.0",
		"eslint-config-prettier": "^10.0.1",
		"eslint-plugin-svelte": "^2.46.1",
		"globals": "^15.14.0",
		"jsdom": "^25.0.1",
		"mdsvex": "^0.12.3",
		"prettier": "^3.4.2",
		"prettier-plugin-svelte": "^3.3.3",
		"prettier-plugin-tailwindcss": "^0.6.11",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tailwind-merge": "^3.0.2",
		"tailwindcss": "^4.0.0",
		"tsconfig-paths": "^4.2.0",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^6.0.0",
		"vitest": "^3.0.0"
	},
	"dependencies": {
		"@faker-js/faker": "^9.8.0",
		"@inlang/paraglide-sveltekit": "^0.15.5",
		"@jorgebodega/typeorm-seeding": "^7.1.0",
		"@node-rs/argon2": "^2.0.2",
		"@oslojs/crypto": "^1.0.1",
		"@oslojs/encoding": "^1.1.0",
		"@sentry/profiling-node": "^9.34.0",
		"@sentry/sveltekit": "^9.34.0",
		"@types/node": "^22.13.4",
		"@types/nodemailer": "^6.4.17",
		"@types/object-hash": "^3.0.6",
		"bullmq": "^5.53.2",
		"class-transformer": "^0.5.1",
		"dotenv": "^16.5.0",
		"lodash": "^4.17.21",
		"nodemailer": "^6.10.0",
		"object-hash": "^3.0.0",
		"pg": "^8.13.3",
		"punycode": "^2.3.1",
		"redis": "^4.7.1",
		"reflect-metadata": "^0.2.2",
		"svelte-portal": "^2.2.1",
		"tailwind-scrollbar": "^4.0.2",
		"ts-punycode": "^0.1.0",
		"tsx": "^4.19.3",
		"typeorm": "^0.3.20",
		"winston": "^3.17.0"
	}
}
